{
  "presets": [
//    "env",  //用process.env获取全局的环境参数
    "stage-1"
  ],
  "plugins": [
    "transform-vue-jsx",  //jsx开发模式
    "syntax-dynamic-import"
  ],
  "env": {
    "browser": {
      "presets": [
        [
          "env", {
          "targets": {
            "browsers": ["last 2 versions", "safari >= 7"]
          }
        }
        ]
      ]
    },
    "node": {
      "presets": [
        [
          "env", {
          "targets": {
            "node": "current"
          }
        }
        ]
      ]
    }
  }
}
